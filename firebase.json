{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ],
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "landing",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],

    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],

    "redirects": [
      {
        "source": "/adhesion",
        "destination": "https://www.helloasso.com/associations/carbonfight/adhesions/adherant",
        "type": 301
      },
      {
        "source": "/dons",
        "destination": "https://www.helloasso.com/associations/carbonfight/collectes/appel-aux-dons-pour-couvrir-les-depenses-de-2024",
        "type": 301
      },
      {
        "source": "/equipe",
        "destination": "https://carbonfight.notion.site/carbonfight/CarbonFight-f3c90f63d36c44a9a1abe5510ebcaffd",
        "type": 301
      },
      {
        "source": "/delete",
        "destination": "https://carbonfight.notion.site/Supprimez-vos-donn-es-e88dc3436e724e6882810d591ee31c95?pvs=4",
        "type": 301
      },
      {
        "source": "/privacy",
        "destination": "https://www.iubenda.com/privacy-policy/77022733",
        "type": 301
      },
      {
        "source": "/cgu",
        "destination": "https://www.iubenda.com/conditions-generales/77022733",
        "type": 301
      } ]
  }
}
