{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ],
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "landing",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
      ],
    "redirects": [
      {
        "source": "/adhesion",
        "destination": "https://www.helloasso.com/associations/carbonfight/adhesions/adherant",
        "type": 301
      },
      {
        "source": "/dons",
        "destination": "https://www.helloasso.com/associations/carbonfight/collectes/appel-aux-dons-pour-couvrir-les-depenses-de-2024",
        "type": 301
      },
      {
        "source": "/equipe",
        "destination": "https://carbonfight.notion.site/Pour-commencer-14cc9ecfe03740af9f94d99e4cb08cfe?pvs=4",
        "type": 301
      },
      {
        "source": "/delete",
        "destination": "https://carbonfight.notion.site/Terms-Privacy-e88dc3436e724e6882810d591ee31c95?pvs=4",
        "type": 301
      } ]
  }
}
